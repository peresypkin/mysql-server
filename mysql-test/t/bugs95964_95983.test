--disable_warnings
DROP TABLE IF EXISTS t0, t1;
--enable_warnings

CREATE TABLE t0(c0 INT);
CREATE TABLE t1(c0 INT) ENGINE = MEMORY;
INSERT INTO t0(c0) VALUES(0);
INSERT INTO t1(c0) VALUES(-1);
SELECT * FROM t0, t1 WHERE (CAST(t1.c0 AS UNSIGNED)) > (IFNULL("u", t0.c0));
SELECT (CAST(t1.c0 AS UNSIGNED)) > (IFNULL("u", t0.c0)) FROM t0, t1;
DROP TABLE t0, t1;

CREATE TABLE t0(c0 INT, c1 TEXT);
INSERT INTO t0(c0, c1) VALUES(-1, "a");
SELECT * FROM t0 WHERE t0.c1 < (t0.c0 & t0.c0) AND t0.c0 = -1;
DROP TABLE t0;
